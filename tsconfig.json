{
	"compilerOptions": {
		"target": "ES2022",
		"module": "CommonJS",
		"rootDir": "src",
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"noImplicitAny": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}